{
  "name": "fartbot",
  "version": "1.0.0",
  "description": "twitch bot",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "gendocs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "test": "node_modules/.bin/mocha -c --recursive src/",
    "cover": "node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- -R spec --recursive ./src/",
    "lint": "node_modules/.bin/eslint src/",
    "pre-commit": "npm run cover && npm run lint"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "lodash": "^4.15.0",
    "tmi.js": "1.1.1"
  },
  "devDependencies": {
    "eslint": "^3.4.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.0",
    "mocha": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fartbagxp/fartbot.git"
  },
  "keywords": [
    "twitch",
    "fart"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fartbagxp/fartbot/issues"
  },
  "homepage": "https://github.com/fartbagxp/fartbot#readme"
}
