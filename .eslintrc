{
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console" : 0,
    "no-var": 1
  },
  "env": {
    "node": true,
    "jasmine" : true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
  }
}